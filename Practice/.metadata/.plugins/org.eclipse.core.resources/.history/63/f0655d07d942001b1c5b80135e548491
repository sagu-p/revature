package eg1;

public class CheckInheritance {
	
	public static void main(String[] args) {
		
		//A >> B >> C
		C obj = new C();
		
		// A >> (B & D)
		D obj1 = new D();
		
		System.out.println("Multilevel Inheritance:");
		obj.show();
		obj.showA();
		obj.showB();
		obj.showC();
		//objB.showB();
		
	}

}
