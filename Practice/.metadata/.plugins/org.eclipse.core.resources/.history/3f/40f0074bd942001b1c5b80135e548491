package eg1;

public class CheckInheritance {
	
	public static void main(String[] args) {
		
		//A >> B >> C
		C obj = new C();
		
		System.out.println("Multilevel Inheritance:");
		obj.show();
		obj.showA();
		obj.showB();
		obj.showC();
		

		// A >> (B & D)
		D obj1 = new D();
		System.out.println("Herarchical Inheritance:");
		System.out.println("A >> D");
		obj1.show();
		obj1.showA();
		obj1.showD();
		
	}

}
