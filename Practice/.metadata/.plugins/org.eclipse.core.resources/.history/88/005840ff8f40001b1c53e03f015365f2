package Exp;

public class TryCatchExp {
	
	public static void main(String[] args) {
		
		int x=10, y=0;
		
		try
		{
			String str = null;
			System.out.println(str.length());
			System.out.println(x/y);
		}
		catch(ArithmaticException ar)
		{
			System.out.println("Arithmatic Exp.");
		}
		catch(Exception e)
		{
			System.out.println("Unhandled Eception.");
		}
		
		
		
	}

}
