package dem1;

public class ChechPoly {
	
	public static void main(String[] args) {
		
		System.out.println("Method Overloading:\n");
		//Methid Overloading
		A a = new A();
		a.show();
		a.show(2611);
		
		System.out.println("Method Overriding:\n");
		
		//Method Overriding
		B o = new B();
		o.show();
	}

}
